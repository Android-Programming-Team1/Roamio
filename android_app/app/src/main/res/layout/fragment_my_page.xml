<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.ui.fragment.MyPageFragment">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#F3FBFF">

        <ImageView
            android:id="@+id/bgCard"
            android:layout_width="320dp"
            android:layout_height="400dp"
            android:layout_marginTop="160dp"
            android:background="@null"
            android:contentDescription="TODO"
            android:src="@drawable/bg_card_rounded"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="ContentDescription,HardcodedText" />
        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="180dp"
            android:src="@drawable/myprofile_icon"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <ImageView
            android:id="@+id/imageView19"
            android:layout_width="145dp"
            android:layout_height="141dp"
            android:layout_marginTop="20dp"
            android:background="@null"
            android:src="@drawable/ic_roamio"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/imageView11" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:text="저는 여행 계획 짜는 일을 좋아해요!"
            android:textAlignment="center"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView19" />

        <ImageButton
            android:id="@+id/imageButton9"
            android:layout_width="320dp"
            android:layout_height="40dp"
            android:background="@null"
            android:scaleType="fitCenter"
            android:src="@drawable/btn_saved_list"
            app:layout_constraintBottom_toTopOf="@+id/imageButton10"
            app:layout_constraintEnd_toEndOf="@+id/bgCard"
            app:layout_constraintStart_toStartOf="@+id/bgCard" />

        <ImageButton
            android:id="@+id/imageButton10"
            android:layout_width="0dp"
            android:layout_height="40dp"
            android:layout_marginBottom="16dp"
            android:background="@null"
            android:scaleType="fitCenter"
            android:src="@drawable/btn_logout"
            app:layout_constraintBottom_toBottomOf="@+id/bgCard"
            app:layout_constraintEnd_toEndOf="@+id/bgCard"
            app:layout_constraintStart_toStartOf="@+id/bgCard" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>
